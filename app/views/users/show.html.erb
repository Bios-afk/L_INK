<!-- Header dashBoard-->
<%= render partial: 'users/header', locals: { user: current_user} %>

<!-- Profile info -->
<% if current_user.userable_type == 'Client' %>
  <%= render partial: 'users/header_profile_client', locals: { user: current_user } %>
  <%= render partial: 'users/body_client', locals: { user: current_user } %>
<% else %>
  <%= render partial: 'users/header_profile_artist', locals: { user: current_user } %>
  <%= render partial: 'artists/artist_profile/body', locals: { artist: current_user.userable } %>
<% end %>
